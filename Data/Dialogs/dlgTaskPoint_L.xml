<?xml version="1.0" encoding="ISO-8859-1"?>

<Form Name="frmTaskPoint" Width="240" Height="320" Caption="Waypoint">
  <Button Caption="Close" X="2" Y="-1" Width="70" Height="22" Font="2" OnClick="OnCloseClicked"/>

  <Canvas Name="frmTaskView" X="2" Y="26" Width="70" Height="70" Font="0" BackColor="0x404000" ForeColor="0xFFFFFF" OnPaint="OnTaskPaint" />

  <Button Name="butRemove" Caption="Remove" X="2" Y="-1" Width="70" Height="22" Font="2" OnClick="OnRemoveClicked"/>
  <Button Name="butRelocate" Caption="Relocate" X="2" Y="-1" Width="70" Height="22" Font="2" OnClick="OnRelocateClicked"/>
  <Button Name="butDown" Caption="Move down" X="2" Y="-1" Width="70" Height="22" Font="2" OnClick="OnMoveAfterClicked"/>
  <Button Name="butUp" Caption="Move up" X="2" Y="-1" Width="70" Height="22" Font="2" OnClick="OnMoveBeforeClicked"/>

  <Panel Name="frmPointCommon" X="80" Y="0" Width="240" Height="80" Font="2">
  <Button Name="butDetails" Caption="Details" X="10" Y="-1" Width="220" Height="22" Font="2" OnClick="OnDetailsClicked"/>
  <Button Name="butType" Caption="Type" X="10" Y="-1" Width="220" Height="22" Font="2" OnClick="OnTypeClicked"/>
  </Panel>

  <Panel Name="frmOZLine" X="80" Y="80" Width="240" Height="142" Font="2">
    <Edit Name="prpOZLineLength" Caption="Length" X="2" Y="-1" Width="240" Height="22" CaptionWidth="150" Font="2" Help="Length of the OZ line.">
      <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min="0" Max="100" Step="0.5" Fine="1" />
    </Edit>
  </Panel>

  <Panel Name="frmOZCylinder" X="80" Y="80" Width="240" Height="142" Font="2">
    <Edit Name="prpOZCylinderRadius" Caption="Radius" X="2" Y="-1" Width="240" Height="22" CaptionWidth="150" Font="2" Help="Radius of the OZ cylinder.">
      <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min="0" Max="100" Step="0.5" Fine="1" />
    </Edit>
  </Panel>

  <Panel Name="frmOZSector" X="80" Y="80" Width="240" Height="142" Font="2">
    <Edit Name="prpOZSectorRadius" Caption="Radius" X="2" Y="-1" Width="240" Height="22" CaptionWidth="150" Font="2" Help="Radius of the OZ sector.">
      <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min="0" Max="100" Step="0.5" Fine="1" />
    </Edit>

    <Edit Name="prpOZSectorStartRadial" Caption="Start radial" X="2" Y="-1" Width="240" Height="22" CaptionWidth="150" Font="2" Help="Start radial of the OZ area">
      <DataField name="" DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" Min="0" Max="360" Step="1"/>
    </Edit>

    <Edit Name="prpOZSectorFinishRadial" Caption="Finish radial" X="2" Y="-1" Width="240" Height="22" CaptionWidth="150" Font="2" Help="Finish radial of the OZ area">
      <DataField name="" DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" Min="0" Max="360" Step="1"/>
    </Edit>
  </Panel>

  <Panel Name="frmOZFAISector" X="80" Y="80" Width="240" Height="142" Font="2">
  </Panel>

</Form>
