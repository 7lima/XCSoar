<Form Width="320" Height="240" Caption="Select Waypoint">
  <Label X="3" Y="0" Width="61" Height="16" Caption="Name" />
  <Button Name="cmdFltName" Caption="*" X="2" Y="18" Width="63"  Height="22" />

  <Edit Name="prpFltDistance" Caption="Distance" X="2" Width="63" Height="36" CaptionWidth="-1">
    <DataField DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDistance"/>
  </Edit>
  <Edit Name="prpFltDirection" Caption="Direction" X="2" Width="63" Height="36" CaptionWidth="-1">
    <DataField DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDirection"/>
  </Edit>

  <Edit Name="prpFltType" Caption="Type" X="2" Width="63" Height="36" CaptionWidth="-1">
    <DataField DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterType"/>
  </Edit>

  <Button Name="cmdSelect" Caption="Select" X="2" Width="63"  Height="35" />
  <Button Name="cmdClose" Caption="Cancel" X="2" Width="63"  Height="35" />

  <List Name="frmWaypointList" X="65" Y="1" Width="-1" Height="-1" ItemHeight="18"/>
</Form>
