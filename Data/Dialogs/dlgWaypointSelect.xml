<Form Name="frmWaypointSelect" Width="240" Height="320" Caption="Select Waypoint">
  <Label X="0" Y="0" Width="75" Height="20" Caption="Filter..." />

  <Label X="2" Y="35" Width="75" Height="18" Caption="Name" />
  <Button Name="cmdFltName" Caption="*" X="2" Y="55" Width="75"  Height="22" />

  <Edit Name="prpFltType" Caption="Type" X="100" Y="39" Width="98" Height="40" CaptionWidth="0">
    <DataField DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterType"/>
  </Edit>

  <Edit Name="prpFltDistance" Caption="Distance" X="2" Y="81" Width="98" Height="40" CaptionWidth="0">
    <DataField DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDistance"/>
  </Edit>

  <Edit Name="prpFltDirection" Caption="Direction" X="100" Y="81" Width="98" Height="40" CaptionWidth="0">
    <DataField DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDirection"/>
  </Edit>

  <List Name="frmWaypointList" X="1" Y="123" Width="-1" Height="-1" ItemHeight="18"/>

  <Button Name="cmdSelect" Caption="Select" X="-154" Y="2" Width="75"  Height="35" />
  <Button Name="cmdClose" Caption="Cancel" X="-77" Y="2" Width="75"  Height="35"  />
</Form>
