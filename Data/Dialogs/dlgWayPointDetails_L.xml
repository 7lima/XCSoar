<?xml version="1.0"?>

<Form Name="frmWayPointInfo" Width="320" Height="240" Caption="Waypoint Info">
  <Button Name="cmdGoto" Caption="Goto" X="5" Y="10" Width="60" Height="28" />

  <SymbolButton Name="cmdPrev" Caption="&lt;" X="5" Y="160" Width="29" Height="28" OnClick="OnPrevClicked" />
  <SymbolButton Name="cmdNext" Caption="&gt;" X="36" Y="160" Width="29" Height="28" OnClick="OnNextClicked" />

  <Button Name="cmdClose" Caption="Close" X="5" Y="190" Width="60" Height="28" />

  <Panel Name="frmInfos" X="70" Y="0" Width="0" Height="222" Border="0">
    <Edit Name="prpWpComment" Caption="Comment" X="2" Y="2" Width="242" Height="40" CaptionWidth="100" ReadOnly="1" />
    <Edit Name="prpLatitude" Caption="Latitude" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpLongitude" Caption="Longitude" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpAltitude" Caption="Elevation" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpSunset" Caption="Sunset" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpDistance" Caption="Distance" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpBearing" Caption="Bearing" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpMc0" Caption="Alt diff Mc 0" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpMc1" Caption="Alt diff Mc safety" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
    <Edit Name="prpMc2" Caption="Alt diff Mc current" X="2" Width="242" Height="20" CaptionWidth="150" ReadOnly="1" />
  </Panel>

  <List Name="frmDetails" X="70" Y="0" Width="0" Height="222" ItemHeight="18"/>

  <Panel Name="frmCommands" X="70" Y="0" Width="246" Height="222" Visible="0" Border="0">
    <Button Name="cmdReplace" Caption="Replace in task" X="50" Y="10" Width="150" Height="30" />
    <Button Name="cmdInserInTask" Caption="Insert in task" X="50" Width="150" Height="30" />
    <Button Name="cmdAppendInTask" Caption="Append to task" X="50" Width="150" Height="30" />
    <Button Name="cmdRemoveFromTask" Caption="Remove from task" X="50" Width="150" Height="30" />
    <Button Name="cmdNewHome" Caption="Set as new home" X="50" Width="150" Height="30" />
    <Button Name="cmdActivatePan" Caption="Pan to waypoint" X="50" Width="150" Height="30" />
  </Panel>

  <Canvas Name="frmImage" X="70" Y="0" Width="0" Height="222" Visible="0" Border="0"/>
</Form>
