<?xml version="1.0" encoding="ISO-8859-1"?>
<PMML version="3.0"
  xmlns="http://www.dmg.org/PMML-3-0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema_instance" >
  <Header copyright="VADIS">
     <Application name="RANK For &lt;you>" version="1.99beta" />
  </Header>
  <Extension extender="RANKF" name="keys"><Key name="urn"></Key></Extension>

<WndForm Name="frmWindSettingRegular" Type="Dialog" X=5 Y=5 Width=153 Height=135 Caption="Wind Settings" Popup=1 Font=2 BackColor=0xDADBAB>
  <WndProperty Name="prpSpeed" Caption="Speed"      X=2 Y=2   Width=145 Height=22 CaptionWidth=68 Font=2 Help="Manual adjustment of wind speed.">
    <DataField name="" DataType="double" DisplayFormat="%.0f %s" EditFormat="%.0f" Min=0 Step=1 OnDataAccess="OnWindSpeedData"/>
  </WndProperty>
  <WndProperty Name="prpDirection" Caption="Direction" X=2 Y=24  Width=145 Height=22 CaptionWidth=68 Font=2 Help="Manual adjustment of wind direction.">
    <DataField name="" DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" Min=0 Max=361 Step=1 OnDataAccess="OnWindDirectionData"/>
  </WndProperty>
  <WndProperty Name="prpAutoWind" Caption="Auto wind"      X=2 Y=46   Width=145 Height=22 CaptionWidth=68 Font=2 Help="This allows switching on or off the automatic wind algorithm.  When the algorithm is switched off, the pilot is responsible for setting the wind estimate.
[Circling] Requires only a GPS source
[ZigZag] requires an intelligent vario with airspeed output.
[Both] Use ZigZag and circling.">
    <DataField name="" DataType="enum" Min=0 Max=50 Step=1/>
  </WndProperty>

  <WndProperty Name="prpTrailDrift" Caption="Trail drift"     X=2 Y=68  Width=145 Height=22 CaptionWidth=68 Font=2 Help="Determines whether the snail trail is drifted with the wind when displayed in circling mode.">
  <DataField name="" DataType="boolean"/>
  </WndProperty>
  <WndButton   Caption="Save"    X=2 Y=90 Width=66  Height=22 Font=2 OnClickNotify="OnSaveClicked"/>
  <WndButton   Caption="Close"    X=74 Y=90 Width=66  Height=22 Font=2 OnClickNotify="OnCloseClicked"/>
</WndForm>

</PMML>