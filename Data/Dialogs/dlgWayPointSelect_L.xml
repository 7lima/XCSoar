<Form Name="frmWayPointSelect" Width="240" Height="320" Caption="Select Waypoint">
  <Label X="0" Y="0" Width="100" Height="20" Font="2" Caption="Filter..." />

  <Label X="2" Y="28" Width="98" Height="18" Font="2" Caption="Name" />
  <Button Name="cmdFltName" Caption="*" X="2" Y="48" Width="98"  Height="22" Font="2" />

  <Edit Name="prpFltType" Caption="Type" X="100" Y="32" Width="98" Height="40" CaptionWidth="0" Font="2">
    <DataField name="" DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterType"/>
  </Edit>

  <Edit Name="prpFltDistance" Caption="Distance" X="2" Y="74" Width="98" Height="40" CaptionWidth="0" Font="2">
    <DataField name="" DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDistance"/>
  </Edit>

  <Edit Name="prpFltDirection" Caption="Direction" X="100" Y="74" Width="98" Height="40" CaptionWidth="0" Font="2">
    <DataField name="" DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDirection"/>
  </Edit>

  <Button Name="cmdSelect" Caption="Select" X="105" Y="2" Width="60"  Height="28" Font="2" />
  <Button Name="cmdClose" Caption="Close" X="167" Y="2" Width="60"  Height="28" Font="2"  />

  <List Name="frmWayPointList" X="1" Y="116" Width="-1" Height="-1" ItemHeight="18" Border="1"/>
</Form>
