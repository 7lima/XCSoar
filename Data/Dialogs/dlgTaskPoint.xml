<?xml version="1.0" encoding="ISO-8859-1"?>

<WndForm Name="frmTaskPoint" Width=320 Height=240 Caption="Waypoint">
  <WndButton   Caption="Close"    X=2 Y=-1 Width=70  Height=22 Font=2 OnClickNotify="OnCloseClicked"/>

  <WndOwnerDrawFrame Name="frmTaskView" X=2 Y=26 Width=70 Height=70 Font=0 BackColor=0x404000 ForeColor=0xFFFFFF OnPaint="OnTaskPaint" />

  <WndButton  Name="butRemove" Caption="Remove"    X=2 Y=-1 Width=70  Height=22 Font=2 OnClickNotify="OnRemoveClicked"/>
  <WndButton  Name="butRelocate"   Caption="Relocate"    X=2 Y=-1 Width=70  Height=22 Font=2 OnClickNotify="OnRelocateClicked"/>
  <WndButton  Name="butDown" Caption="Move down"    X=2 Y=-1 Width=70  Height=22 Font=2 OnClickNotify="OnMoveAfterClicked"/>
  <WndButton  Name="butUp" Caption="Move up"    X=2 Y=-1 Width=70  Height=22 Font=2 OnClickNotify="OnMoveBeforeClicked"/>

  <Panel Name="frmPointCommon" X=80 Y=0 Width=240 Height=80 Font=2>
  <WndButton  Name="butDetails" Caption="Details"    X=142 Y=-1 Width=70  Height=22 Font=2 OnClickNotify="OnDetailsClicked"/>
  <WndButton  Name="butType" Caption="Type"    X=142 Y=-1 Width=70  Height=22 Font=2 OnClickNotify="OnTypeClicked"/>
  </Panel>

  <Panel Name="frmOZLine" X=80 Y=80 Width=240 Height=142 Font=2>
    <WndProperty Name="prpOZLineLength" Caption="Length"      X=2 Y=-1   Width=240 Height=22 CaptionWidth=150 Font=2 Help="Length of the OZ line.">
      <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min=0 Max=100 Step="0.5" Fine=1 />
    </WndProperty>
  </Panel>

  <Panel Name="frmOZCylinder" X=80 Y=80 Width=240 Height=142 Font=2>
    <WndProperty Name="prpOZCylinderRadius" Caption="Radius"      X=2 Y=-1   Width=240 Height=22 CaptionWidth=150 Font=2 Help="Radius of the OZ cylinder.">
      <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min=0 Max=100 Step="0.5" Fine=1 />
    </WndProperty>
  </Panel>

  <Panel Name="frmOZSector" X=80 Y=80 Width=240 Height=142 Font=2>
    <WndProperty Name="prpOZSectorRadius" Caption="Radius"      X=2 Y=-1   Width=240 Height=22 CaptionWidth=150 Font=2 Help="Radius of the OZ sector.">
      <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min=0 Max=100 Step="0.5" Fine=1 />
    </WndProperty>

    <WndProperty Name="prpOZSectorStartRadial" Caption="Start radial"      X=2 Y=-1   Width=240 Height=22 CaptionWidth=150 Font=2 Help="Start radial of the OZ area">
      <DataField name="" DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" Min=0 Max=360 Step=1/>
    </WndProperty>

    <WndProperty Name="prpOZSectorFinishRadial" Caption="Finish radial"      X=2 Y=-1   Width=240 Height=22 CaptionWidth=150 Font=2 Help="Finish radial of the OZ area">
      <DataField name="" DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" Min=0 Max=360 Step=1/>
    </WndProperty>
  
  </Panel>

  <Panel Name="frmOZFAISector" X=80 Y=80 Width=240 Height=142 Font=2>
  </Panel>

</WndForm>