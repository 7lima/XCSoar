<Form Name="frmWayPointSelect" Width="320" Height="240" Caption="Select Waypoint">
  <Label X="0" Y="0" Width="61" Height="20" Font="2" Caption="Filter..." />

  <Label X="3" Y="20" Width="61" Height="16" Font="2" Caption="Name" />
  <Button Name="cmdFltName" Caption="*" X="3" Y="38" Width="63"  Height="22" Font="2" />

  <Edit Name="prpFltDistance" Caption="Distance" X="2" Y="-1" Width="63" Height="36" CaptionWidth="0" Font="2" >
    <DataField name="" DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDistance"/>
  </Edit>
  <Edit Name="prpFltDirection" Caption="Direction" X="2" Y="-1" Width="63" Height="36" CaptionWidth="0" Font="2" >
    <DataField name="" DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterDirection"/>
  </Edit>

  <Edit Name="prpFltType" Caption="Type" X="2" Y="-1" Width="63" Height="36" CaptionWidth="0" Font="2" >
    <DataField name="" DataType="enum" DisplayFormat="%s" EditFormat="%s" OnDataAccess="OnFilterType"/>
  </Edit>

  <Button Name="cmdSelect" Caption="Select" X="2" Y="-1" Width="63"  Height="28" Font="2" />
  <Button Name="cmdClose" Caption="Close" X="2" Y="-1" Width="63"  Height="28" Font="2" />

  <List Name="frmWayPointList" X="65" Y="1" Width="-1" Height="-1" ItemHeight="18" Border="1"/>
</Form>
