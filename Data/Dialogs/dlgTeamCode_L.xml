<?xml version="1.0" encoding="ISO-8859-1"?>

<Form Name="frmTeamCode" Width="206" Height="240" Caption="Team code">
  <Edit Name="prpOwnCode" Caption="Own code:" X="2"  Width="200" Height="22" CaptionWidth="100" ReadOnly="1" />

  <Edit Name="prpMateCode" Caption="Mate code:" X="2"  Width="200" Height="22" CaptionWidth="100" ReadOnly="1" />

  <Edit Name="prpRange" Caption="Range:" X="2"  Width="200" Height="22" CaptionWidth="100" ReadOnly="1">
    <DataField DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" ReadOnly="1" />
  </Edit>

  <Edit Name="prpBearing" Caption="Bearing:" X="2"  Width="200" Height="22" CaptionWidth="100" ReadOnly="1">
    <DataField DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" ReadOnly="1" />
  </Edit>

  <Edit Name="prpRelBearing" Caption="Rel. Bearing:" X="2"  Width="200" Height="22" CaptionWidth="100" ReadOnly="1" />

  <Edit Name="prpFlarmLock" Caption="Flarm lock:" X="2"  Width="200" Height="22" CaptionWidth="100" ReadOnly="1" />

  <Button Caption="Flarm Lock" X="5" Y="140" Width="90"  Height="40" OnClick="OnFlarmLockClicked" />

  <Button Name="cmdSetCode" Caption="Set Mate code" X="95" Y="140" Width="90"  Height="40" />

  <Button Caption="Close" X="5" Y="185" Width="180"  Height="35" OnClick="OnCloseClicked" />
</Form>
