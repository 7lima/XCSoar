  <SymbolButton Name="cmdPrev" Caption="&lt;" X="0" Y="240" Width="35" Height="28" OnClick="OnPrev" />
  <Button Name="cmdClose" Caption="Close" X="5" Y="270" Width="60" Height="28" OnClick="OnClose" />

  <Tabbed Name="tab" X="0" Y="0" Width="0" Height="222">
    <Panel Name="frmInfo">
      <Canvas X="2" Y="2" Width="-2" OnPaint="OnLogoPaint" />
    </Panel>
  
    <Panel Name="frmAuthors">
      <Edit Name="prpAuthors" X="2" Y="0" CaptionWidth="-1" ReadOnly="1" MultiLine="1" />
    </Panel>
  
    <Panel Name="frmLicense">
      <Edit Name="prpLicense" X="2" Y="0" CaptionWidth="-1" ReadOnly="1" MultiLine="1" />
    </Panel>
  </Tabbed>
</Form>
