  <Extension extender="RANKF" name="keys"><Key name="urn"></Key></Extension>

<WndForm Name="frmVegaDemo" Type="Dialog" X=5 Y=5 Width=163 Height=160 Caption="Vario Demo" Font=2 BackColor=0xDADBAB>

  <WndButton   Caption="Close"    X=78 Y=2 Width=70  Height=22 Font=2 OnClickNotify="OnCloseClicked">
  </WndButton> 

  <WndProperty name="prpVegaDemoW" Caption="TE vario"  X=2 Y=-1  Width=155 Height=22 CaptionWidth=78 Font=2>
    <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min=-10 Max=10 Step=0.1  OnDataAccess="OnVegaDemoW"/>
  </WndProperty>

  <WndProperty name="prpVegaDemoV" Caption="Airspeed"  X=2 Y=-1  Width=155 Height=22 CaptionWidth=78 Font=2>
    <DataField name="" DataType="double" DisplayFormat="%.0f %s" EditFormat="%.0f" Min=0 Max=200 Step=2  OnDataAccess="OnVegaDemoV"/>
  </WndProperty>

  <WndProperty name="prpVegaDemoV" Caption="Circling"  X=2 Y=-1  Width=155 Height=22 CaptionWidth=78 Font=2>
    <DataField name="" DataType="boolean" OnDataAccess="OnVegaDemoAudioClimb"/>
  </WndProperty>
  
</WndForm>    
