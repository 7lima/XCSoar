<?xml version="1.0" encoding="ISO-8859-1"?>
<PMML version="3.0"
  xmlns="http://www.dmg.org/PMML-3-0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema_instance" >
  <Header copyright="VADIS">
     <Application name="RANK For &lt;you>" version="1.99beta" />
  </Header>
  <Extension extender="RANKF" name="keys"><Key name="urn"></Key></Extension>

<WndForm Name="frmWindSettingRegular" Type="Dialog" X=5 Y=5 Width=153 Height=133 Caption="Wind Settings" Font=2 BackColor=0xDADBAB>
  <WndButton   Caption="Close"    X=68 Y=2 Width=60  Height=22 Font=2 OnClickNotify="OnCloseClicked"/>
  <WndProperty Name="prpSpeed" Caption="Speed"      X=2 Y=-1   Width=145 Height=22 CaptionWidth=68 Font=2>
    <DataField name="" DataType="double" DisplayFormat="%.0f %s" EditFormat="%.0f" Min=0 Step=1 OnDataAccess="OnWindSpeedData"/>
  </WndProperty>
  <WndProperty Name="prpDirection" Caption="Direction" X=2 Y=-1  Width=145 Height=22 CaptionWidth=68 Font=2 >
    <DataField name="" DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" Min=0 Max=361 Step=1 OnDataAccess="OnWindDirectionData"/>
  </WndProperty>
  <WndProperty Name="prpAutoWind" Caption="Auto wind"      X=2 Y=-1   Width=145 Height=22 CaptionWidth=68 Font=2>
    <DataField name="" DataType="boolean"/>
  </WndProperty>
  <WndButton   Caption="Save"    X=68 Y=-1 Width=60  Height=22 Font=2 OnClickNotify="OnSaveClicked"/>
  
</WndForm>    

<WndForm Name="frmWindSettingBig" Type="Dialog" X=5 Y=5 Width=230 Height=220 Caption="Basic Settings" Font=2 BackColor=0xDADBAB>

  
</WndForm>    

</PMML>