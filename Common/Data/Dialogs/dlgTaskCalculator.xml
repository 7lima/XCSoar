<?xml version="1.0" encoding="ISO-8859-1"?>
<PMML version="3.0"
  xmlns="http://www.dmg.org/PMML-3-0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema_instance" >
  <Header copyright="VADIS">
     <Application name="RANK For &lt;you>" version="1.99beta" />
  </Header>
  <Extension extender="RANKF" name="keys"><Key name="urn"></Key></Extension>

<WndForm Name="frmTaskCalculator" Type="Dialog" X=5 Y=5 Width=240 Height=160 Caption="Task Calculator" Font=2  BackColor=0xDADBAB>

  <WndButton   Caption="OK"    X=2 Y=2 Width=60  Height=22 Font=2 OnClickNotify="OnOKClicked" Tag=1/>
  <WndButton   Caption="Cancel"    X=65 Y=2 Width=60  Height=22 Font=2 OnClickNotify="OnCancelClicked" Tag=2/>

  <WndProperty Name="prpAATTime" Caption="Assigned task time" X=2 Y=-1  Width=238 Height=22 CaptionWidth=150 Font=2 ReadOnly=1 min=0 max=600 step=10>
    <DataField name="" DataType="double" DisplayFormat="%.0f min" EditFormat="%.0f" readonly=1/>
  </WndProperty>

  <WndProperty Name="prpAATEst" Caption="Estimated task time" X=2 Y=-1  Width=238 Height=22 CaptionWidth=150 Font=2 ReadOnly=1 min=0 max=600 step=10>
    <DataField name="" DataType="double" DisplayFormat="%.0f min" EditFormat="%.0f" readonly=1/>
  </WndProperty>

  <WndProperty Name="prpDistance" Caption="Task distance" X=2 Y=-1  Width=238 Height=22 CaptionWidth=150 Font=2 ReadOnly=1 min=0 max=2000 step=10 >
    <DataField name="" DataType="double" DisplayFormat="%.0f %s" EditFormat="%.0f" readonly=1/>
  </WndProperty>

  <WndProperty Name="prpMacCready" Caption="MacCready"      X=2 Y=-1   Width=238 Height=22 CaptionWidth=150 Font=2 Help="Adjusts Mc value used in the calculator.  Use this to determine the effect on estimated task time due to changes in conditions.  This value will not affect the main computer's setting if the dialog is exited with the Cancel button.">
    <DataField name="" DataType="double" DisplayFormat="%.1f %s" EditFormat="%.1f" Min=0 Max=10 Step=0.1 OnDataAccess="OnMacCreadyData"/>
  </WndProperty>

  <WndProperty Name="prpRange" Caption="Range"      X=2 Y=-1   Width=238 Height=22 CaptionWidth=150 Font=2 Help="For AAT tasks, this setting can be used to adjust the target points within the AAT sectors.  Larger values move the target points to produce larger task distances, smaller values move the target points to produce smaller task distances.">
    <DataField name="" DataType="double" DisplayFormat="%.0f%%" EditFormat="%.0f" Min=-100 Max=100 Step=1 OnDataAccess="OnRangeData"/>
  </WndProperty>

</WndForm>

</PMML>