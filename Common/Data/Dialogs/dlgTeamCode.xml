  <Extension extender="RANKF" name="keys"><Key name="urn"></Key></Extension>

<WndForm Name="frmTeamCode" Type="Dialog" X=5 Y=5 Width=150 Height=135 Caption="Team code" Font=2 BackColor=0xDADBAB>

  <WndButton   Caption="Close"    X=5 Y=2 Width=70  Height=22 Font=2 OnClickNotify="OnCloseClicked">
  </WndButton>

  <WndProperty Name="prpCode" Caption="Own code" X=2 Y=-1  Width=155 Height=22 CaptionWidth=78 Font=2 ReadOnly=1 />

  <WndButton Name="cmdCode" Caption="Mate code: " X=5 Y=-1 Width=135  Height=22 Font=2 />

  <WndProperty Name="prpRange" Caption="Range" X=2 Y=-1  Width=155 Height=22 CaptionWidth=78 Font=2 ReadOnly=1>
    <DataField name="" DataType="double" DisplayFormat="%.0f %s" EditFormat="%.0f" ReadOnly=1 />
  </WndProperty>

  <WndProperty Name="prpBearing" Caption="Bearing" X=2 Y=-1  Width=155 Height=22 CaptionWidth=78 Font=2 ReadOnly=1>
    <DataField name="" DataType="double" DisplayFormat="%.0f°" EditFormat="%.0f" ReadOnly=1 />
  </WndProperty>

</WndForm>

