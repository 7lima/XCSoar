<?xml version="1.0" encoding="ISO-8859-1"?>
<PMML version="3.0"
  xmlns="http://www.dmg.org/PMML-3-0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema_instance" >
  <Header copyright="VADIS">
     <Application name="RANK For &lt;you>" version="1.99beta" />
  </Header>
  <Extension extender="RANKF" name="keys"><Key name="urn"></Key></Extension>

<WndForm Name="frmWaypointEdit" Type="Dialog" X=0 Y=0 Width=320 Height=240 Caption="Waypoint Editor" Font=2 BackColor=0xDADBAB>

  <WndButton   Caption="Close"    X=2 Y=2 Width=60  Height=22 Font=2 OnClickNotify="OnCloseClicked"/>

  <WndButton Name="cmdName" Caption="Name: " X=70 Y=0 Width=170  Height=28 Font=2 Tag=4 />

  <WndButton Name="cmdComment" Caption="Comment: " X=70 Y=-1 Width=170  Height=28 Font=2 Tag=4 />
 
  <WndProperty Name="prpLongitudeD" Caption="Longitude E" X=2 Y=70 Width=140 Height=28 CaptionWidth=90 Font=2 >
      <DataField name="" DataType="double" DisplayFormat="%.0fd" EditFormat="%.0f" Min=-180 Max=179 Step=1/>
  </WndProperty>
  <WndProperty Name="prpLongitudeM" Caption=" " X=145 Y=70 Width=60 Height=28 CaptionWidth=15 Font=2 >
      <DataField name="" DataType="double" DisplayFormat="%.0fm" EditFormat="%.0f" Min=0 Max=59 Step=1/>
  </WndProperty>
  <WndProperty Name="prpLongitudeS" Caption=" " X=215 Y=70 Width=60 Height=28 CaptionWidth=15 Font=2 >
      <DataField name="" DataType="double" DisplayFormat="%.0fs" EditFormat="%.0f" Min=0 Max=59 Step=1/>
  </WndProperty>

  <WndProperty Name="prpLatitudeD" Caption="Latitude N" X=2 Y=100 Width=140 Height=28 CaptionWidth=90 Font=2 >
      <DataField name="" DataType="double" DisplayFormat="%.0fd" EditFormat="%.0f" Min=-89 Max=89 Step=1/>
  </WndProperty>
  <WndProperty Name="prpLatitudeM" Caption=" " X=145 Y=100 Width=60 Height=28 CaptionWidth=15 Font=2 >
      <DataField name="" DataType="double" DisplayFormat="%.0fm" EditFormat="%.0f" Min=0 Max=59 Step=1/>
  </WndProperty>
  <WndProperty Name="prpLatitudeS" Caption=" " X=215 Y=100 Width=60 Height=28 CaptionWidth=15 Font=2 >
      <DataField name="" DataType="double" DisplayFormat="%.0fs" EditFormat="%.0f" Min=0 Max=59 Step=1/>
  </WndProperty>

  <WndProperty Name="prpAltitude" Caption="Altitude" X=2 Y=-1 Width=180 Height=28 CaptionWidth=90 Font=2 >
      <DataField name="" DataType="double" DisplayFormat="%.0f %s" EditFormat="%.0f" Min=0 Max=30000 Step=5/>
  </WndProperty>

  <WndProperty Name="prpFlags" Caption="Type"      X=2 Y=-1   Width=180 Height=28 CaptionWidth=90 Font=2 >
    <DataField name="" DataType="enum" Min=0 Max=10 Step=1 />
  </WndProperty>

</WndForm>    

</PMML>