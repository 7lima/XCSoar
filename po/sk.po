msgstr ""
"Podiel celkového zisku/straty výšky v poslednom stupáku k času strávenému "
"krúžením."

#: src/InfoBoxes/Content/Factory.cpp:119
#, no-c-format
msgid "Last Thermal Gain"
msgstr "Zisk posledného stupáku"

#: src/InfoBoxes/Content/Factory.cpp:120
#, no-c-format
msgid "TL Gain"
msgstr "SP zisk"

#: src/InfoBoxes/Content/Factory.cpp:121
#, no-c-format
msgid "Total altitude gain/loss in the last thermal."
msgstr "Celkový zisk/strata výšky v poslednom stupáku."

#: src/InfoBoxes/Content/Factory.cpp:127
#, no-c-format
