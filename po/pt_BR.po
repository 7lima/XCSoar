msgstr "Ganho/perda de altitude na última térmica divido pelo tempo girando."

#: src/InfoBoxes/Content/Factory.cpp:158
#, no-c-format
msgid "Last thermal gain"
msgstr "Último ganho térmico"

#: src/InfoBoxes/Content/Factory.cpp:159
#, no-c-format
msgid "TL Gain"
msgstr "TL Ganho"

#: src/InfoBoxes/Content/Factory.cpp:160
#, no-c-format
